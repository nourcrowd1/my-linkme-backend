<link rel="preconnect" href="https://fonts.gstatic.com">
<link href="https://fonts.googleapis.com/css2?family=Karla&display=swap" rel="stylesheet">
{% endblock %}

{% block styles %}
<style amp-custom>
    body{
      font-family:'Karla', sans-serif;
      display:flex;
      flex-direction:column;
      align-items: center;
      background: #fafafa;
      font-size:16px;
      line-height:24px;
      color: black;
    }
    h1 {
        font-weight: 800;
        font-size: 72px;
        line-height: 1.2;
        margin: 0;
    }

    h2 {
        font-weight: 800;
        font-size: 48px;
        line-height: 1.2;
        margin: 0;
    }
    a {
        text-decoration: none;
        color: black;
    }
    .header {
        width: 100%;
        height: 72px;
        display:flex;
        justify-content: center;
        align-items: center;
        box-shadow: 0 1px 0 #E5E9F2;
    }
    .header-content {
        display:flex;
        justify-content: space-between;
    }

    .content {
        max-width: 1110px;
        width: 100%;
    }

    .header-links {
        display:flex;
        align-items: center;
    }

    .header-links .link {
        margin-left: 32px;
        display: flex;
        font-weight: bold;
        font-size: 14px;
    }

    .logo {
        display: flex;
        font-weight: bold;
        font-size: 14px;
    }

    .button {
        padding: 10px 24px;
        text-align: center;
        font-weight: bold;
        font-size: 14px;
        background: #E82B5A;
        border-radius: 8px;
        color: white;
    }
    .button.bigger {
        padding: 16px 40px;
        font-size: 20px;
    }

    .content-block {
        width: 100%;
        display: flex;
        align-items: center;
        justify-content: center;
    }

    .full-page-height {
        height: 100%;
        min-height: calc(100vh - 72px);
    }

    .content-block.column {
        padding: 50px 0;
        flex-direction: column;
        text-align: center;
    }

    .block {
        width: 100%;
        display: flex;
        justify-content: space-between;
        align-items: stretch;
    }

    .text-block, .image-block {
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: flex-start;
        flex: 1;
    }

    .text-block.center {
        width: 100%;
        max-width: 730px;
        align-items: center;
    }

    .text-block .button {
        margin-top: 20px;
    }

    .image-block.right {
        align-items: flex-end;
    }

    .text {
        margin-top: 24px;
        font-weight: normal;
        font-size: 18px;
        line-height: 32px;
    }

    .usps {
        justify-content: center;
        margin-top: 50px;
    }

    .usp {
        display: flex;
        flex-direction: column;
        align-items: center;
    }

    .usp svg{
        width: 24px;
        height: 24px;
    }

    .usp h3{
        margin-top: 24px;
        margin-bottom: 0;
        font-weight: 600;
        font-size: 24px;
    }

    .usp span{
        margin-top: 50px;
        font-weight: normal;
        font-size: 16px;
    }

    .footer {
        width: 100%;
        background: #364152;
        padding: 48px 24px 24px 24px;
        display: flex;
        flex-direction:column;
        align-items: center;
    }

    .footer-links {
        display: flex;
        justify-content: space-between;
        width: 100%;
        max-width: 730px;
        margin-top: 24px;
    }

    .footer-links a {
        font-weight: normal;
        font-size: 16px;
        color: #FFFFFF
    }

    .footer-divider {
        height: 1px;
        width: 100%;
        max-width: 730px;
        background: #FFFFFF;
        opacity: 0.2;
        margin-top: 48px;
    }

    .footer-copyright {
        margin-top: 24px;
        font-weight: normal;
        font-size: 14px;
        color: #D9DBE1;
    }

    @media screen and (max-width: 601px) {
        .block {
            flex-direction: column;
        }
        .block.reverse {
            flex-direction: column-reverse;
        }
    }
</style>
{% endblock %}